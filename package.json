{
    "name": "round-assign",
    "version": "1.0.0",
    "private": true,
    "description": "A Probot app that adds reviewers to pull requests when pull requests are opened.",
    "homepage": "https://github.com/mulesoft/auto-assign",
    "keywords": [
        "probot",
        "github",
        "probot-app"
    ],
    "scripts": {
        "build": "tsc",
        "start": "probot run ./lib/index.js",
        "dev": "nodemon --config nodemon.json src/probot-run.ts",
        "dev:debug": "nodemon --config nodemon.json --inspect-brk src/probot-run.ts",
        "test": "jest --coverage",
        "test:watch": "jest --coverage --watch --notify --notifyMode=change",
        "lint": "tslint -c tslint.json --project .",
        "format": "prettier --write .",
        "format:check": "prettier --check ."
    },
    "dependencies": {
        "pg": "8.6.0",
        "probot": "11.3.0"
    },
    "devDependencies": {
        "@types/generic-pool": "3.1.9",
        "@types/jest": "26.0.23",
        "@types/node": "14.14.43",
        "@types/pg": "7.14.11",
        "jest": "26.6.3",
        "nock": "13.0.11",
        "nodemon": "2.0.7",
        "prettier": "2.2.1",
        "smee-client": "1.2.2",
        "ts-jest": "26.5.5",
        "ts-node": "9.1.1",
        "tslint": "5.20.1",
        "tslint-config-prettier": "1.18.0",
        "tslint-config-standard": "9.0.0",
        "typescript": "4.2.4"
    },
    "engines": {
        "node": ">= 15.11.0",
        "npm": ">= 7.6.0"
    }
}
